    \section*{Dictionnaire}
    \subsection*{Conception préliminaire}
    \subsubsection*{Signatures}

	\begin{itemize}[label=$\ $, leftmargin=1cm]
		 \item \textbf{procédure} ajoutMot (E/S dictionnaire  :Dictionnaire,E UnMot:Mot)
		 \item \textbf{procédure} supprimeMot (E/S dictionnaire :Dictionnaire,E UnMot :Mot)
		         \item \textbf{Préconditions } supprimeMot (dictionnaire, UnMot) : estPresent(dictionnaire)
		 \item \textbf{fonction} estVide(dictionnaire : Dictionnaire):Booléen
		 \item \textbf{fonction} estPresent(dictionnaire : Dictionnaire, UnMot:Mot):Booléen
		            \item \textbf{Préconditions } estPresent(dictionnaire, UnMot) : non estVide(dictionnaire)
	\end{itemize}
	

